<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <LangVersion>latest</LangVersion>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>cdorst</Authors>
    <Copyright>Copyright Â© Christopher Dorst</Copyright>
    <Description>Metapackage for Microsoft Azure Storage dependencies</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIconUrl>https://cdorst-dev.azureedge.net/nuget/package-icon-64.png</PackageIconUrl>
    <PackageId>CDorst.Azure.Storage.Metapackage</PackageId>
    <PackageLicenseUrl>https://github.com/cdorst/Azure.Storage.Metapackage/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/cdorst/Azure.Storage.Metapackage</PackageProjectUrl>
    <RepositoryUrl>https://github.com/cdorst/Azure.Storage.Metapackage</RepositoryUrl>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="WindowsAzure.Storage" Version="9.1.0" />
  </ItemGroup>

</Project>
